{
  "source": {
    "include": [
      "app.js",
      "server",
      "public/js/modules",
      "docs"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules|development)"
  },
  "opts": {
    "destination": "./public/docs",
    "recurse": true,
    "readme": "./README.md",
    "template": "./docs/jsdoc-template/docdash",
    "encoding": "utf8",
    "tutorials": "./docs/tutorials"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "default": {
      "outputSourceFiles": true,
      "includeDate": true
    },
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "sectionOrder": [
      "Tutorials",
      "Classes",
      "Modules",
      "Namespaces",
      "Global"
    ]
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}
